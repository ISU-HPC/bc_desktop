---
batch_connect:
  template: vnc

script:
  accounting_id: "<%= custom_account %>"
  native:
    - "--nodes=<%= bc_num_slots.blank? ? 1 : bc_num_slots.to_i %>"
    - "--mem=<%= mem_req %>" 
    - "--job-name=<%= job_name %>"
    - "--ntasks=<%= num_cores %>"
    - "--account=<%= custom_account.delete(' ') %>"
    - "--partition=<%= custom_queue.delete(' ') %>"
